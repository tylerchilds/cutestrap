{
  "name": "cutestrap",
  "author": "Tyler Childs",
  "contributors": [
    "Tyler Childs <tylerwchilds@gmail.com>"
  ],
  "description": "A sassy, opinionated CSS Framework. A tiny alternative to Bootstrap.",
  "homepage": "https://www.cutestrap.com/",
  "version": "1.2.3",
  "repository": {
    "type": "git",
    "url": "git://github.com/cutestrap/cutestrap.git"
  },
  "devDependencies": {
    "bower": "latest",
    "gulp": "latest",
    "gulp-bump": "latest",
    "gulp-clean": "latest",
    "gulp-concat": "latest",
    "gulp-cssmin": "latest",
    "gulp-exec": "latest",
    "gulp-git": "latest",
    "gulp-include": "latest",
    "gulp-rename": "latest",
    "gulp-replace": "latest",
    "gulp-sass": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-stylelint": "latest",
    "gulp-swig": "latest",
    "gulp-tag-version": "latest",
    "gulp-zip": "latest",
    "kss": "^2.1.0",
    "minimist": "latest",
    "run-sequence": "latest"
  },
  "stylelint": {
    "rules": {
      "block-no-empty": null,
      "color-no-invalid-hex": true,
      "declaration-colon-space-after": "always",
      "indentation": ["tab", {
        "except": ["value"]
      }],
      "max-empty-lines": 2,
      "unit-whitelist": ["em", "rem", "%", "s"]
    }
  }
}
